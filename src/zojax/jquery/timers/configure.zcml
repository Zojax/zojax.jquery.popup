<configure
   xmlns="http://namespaces.zope.org/zope"
   xmlns:zojax="http://namespaces.zope.org/zojax">

  <!-- resources -->
  <zojax:resourcedirectory
     name="jquery-timers"
     directory="resources"
     mapping=".css:zrt" />

  <zojax:resourceinclude
     name="jquery.timers.js"
     library="jquery-plugins" require="jquery"
     type="javascript" path="jquery-timers/jquery.timers.js"
     compress="yes" />
       

</configure>
